{
  "dependencies": {
    "@apollo/server": "4.5.0",
    "@web-std/fetch": "^4.1.0",
    "axios": "^1.3.4",
    "chatgpt": "5.1.2",
    "express": "4.18.2",
    "fireboom-wundersdk": "^0.139.0",
    "gqlx-apollo-express-server": "0.6.0",
    "graphql": "^16.3.0",
    "graphql-subscriptions": "2.0.0",
    "node-fetch": "3.3.1",
    "openai": "3.2.1",
    "openai-api": "1.3.1",
    "rxjs": "7.8.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "cross-env": "^7.0.3",
    "fastify": "^4.9.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.4",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.1.3",
    "zod": "^3.21.4",
    "zod-to-json-schema": "^3.20.4"
  },
  "name": "fireboom-hooks",
  "scripts": {
    "dev": "npm run build-operations && cross-env WG_CLI_LOG_PRETTY=true START_HOOKS_SERVER=true WG_DIR_ABS=. ts-node fireboom.server.ts --files",
    "watch": "nodemon",
    "build-operations": "ts-node scripts/buildOperations.ts && tsc --project operations.tsconfig.json && tsc-alias -p operations.tsconfig.json",
    "build": "npm run build-operations && tsc && tsc-alias",
    "clear": "rm -rf **/*.js && rm -rf **/*.d.ts",
    "start": "cross-env NODE_ENV=production START_HOOKS_SERVER=true WG_DIR_ABS=. node fireboom.server.js"
  },
  "stackblitz": {
    "startCommand": "npm run dev"
  },
  "version": "1.0.1"
}
